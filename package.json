{
  "name": "os-elements",
  "version": "0.0.0-semantic-release",
  "description": "Reusable Angular1 components, from Ordnance Survey",
  "homepage": "http://ordnancesurvey.github.io/web-elements",
  "repository": {
    "type": "git",
    "url": "https://github.com/OrdnanceSurvey/web-elements.git"
  },
  "contributors": [],
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run clean:build",
    "build": "npm run tslint && webpack && npm run build:css",
    "prebuild:css": "npm run clean:css",
    "build:css": "find src -type f -name '*.styl' -exec ./node_modules/.bin/stylus --include-css {} +",
    "postbuild:css": "find src -type f -name '*.css' -exec cat {} > dist/os-elements.css +",
    "prebuild:docs": "rm -rf dist/docs",
    "build:docs": "./tasks/docs-copy-components.sh && webpack --config docs/docs.webpack.config.js && npm run build:docs-css",
    "build:docs-css": "find docs -type f -name '*.styl' -exec ./node_modules/.bin/stylus --include-css {} +",
    "postbuild:docs-css": "find docs -type f -name '*.css' -exec cat {} > dist/docs/os-elements-docs.css +",
    "clean:build": "rm -rf build",
    "clean:css": "rm -f dist/os-elements.css; find src -type f -name '*.css' -exec rm {} +",
    "test": "./node_modules/.bin/karma start ./karma.config.js",
    "test-ci": "./node_modules/.bin/karma start ./karma-ci.config.js",
    "test-watcher": "./node_modules/.bin/karma start ./karma.config.js --no-single-run",
    "tslint": "./node_modules/.bin/tslint \"src/**/*.ts\" --exclude \"**/*.d.ts\"",
    "semantic-release": "./node_modules/.bin/semantic-release pre && npm publish && ./node_modules/.bin/semantic-release post",
    "docs-deploy": "tasks/docs-deploy.sh",
    "dev": "parallelshell 'npm run live-reload' 'npm run watch:build'",
    "watch:css": "./node_modules/.bin/watch-run -p 'src/**/*.styl' -i npm run build:css",
    "watch:lint": "./node_modules/.bin/watch-run -p 'src/**/*.ts' npm run tslint",
    "watch:build": "./node_modules/.bin/watch-run --initial -p 'src/**/*.*' npm run build",
    "watch:docs": "npm run watch:docs-html",
    "watch:docs-html": "./node_modules/.bin/watch-run -p 'src/**/*' npm run tslint",
    "live-reload": "./node_modules/.bin/livereload --port 9091 dist/"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/rx": "^2.5.34",
    "material-design-lite": "1.1.x",
    "rx": "4.0.x",
    "rx-angular": "1.1.x",
    "string-replace-webpack-plugin": "^0.0.4",
    "webpack-livereload-plugin": "^0.9.0"
  },
  "devDependencies": {
    "@types/angular": "1.5.8",
    "@types/angular-material": "^1.1.42",
    "@types/lodash": "^4.14.43",
    "@types/openlayers": "^3.18.40",
    "angular": "1.5.8",
    "angular-animate": "1.5.8",
    "angular-aria": "1.5.8",
    "angular-material": "1.1.0",
    "angular-messages": "1.5.8",
    "angular-mocks": "1.5.8",
    "angular-sanitize": "1.5.8",
    "angular-ui-router": "^0.3.2",
    "awesome-typescript-loader": "2.2.4",
    "condition-circle": "1.2.x",
    "css-loader": "0.23.x",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "1.0.x",
    "flatdoc": "^0.9.0",
    "html-loader": "0.4.x",
    "html-webpack-plugin": "^2.24.1",
    "imports-loader": "^0.7.0",
    "istanbul-instrumenter-loader": "0.2.x",
    "jasmine": "2.x",
    "jasmine-core": "2.x",
    "jquery": "1.9.1",
    "karma": "0.13.x",
    "karma-chrome-launcher": "0.x",
    "karma-coverage": "0.x",
    "karma-coveralls": "1.x",
    "karma-jasmine": "0.x",
    "karma-jasmine-matchers": "2.x",
    "karma-nested-reporter": "0.x",
    "karma-phantomjs-launcher": "1.x",
    "karma-sauce-launcher": "0.3.x",
    "karma-sourcemap-loader": "0.x",
    "karma-threshold-reporter": "0.x",
    "karma-webpack": "1.x",
    "livereload": "^0.6.0",
    "ng-annotate-loader": "^0.2.0",
    "oclazyload": "^1.0.9",
    "openlayers": "3.18.2",
    "parallelshell": "^2.0.0",
    "phantomjs-prebuilt": "2.1.x",
    "pug": "^2.0.0-beta6",
    "semantic-release": "4.3.x",
    "stylus": "^0.54.5",
    "template-html-loader": "0.0.x",
    "tslint": "^4.0.2",
    "typescript": "2.1.1",
    "watch-run": "^1.2.4",
    "webpack": "1.13.2"
  },
  "peerDependencies": {
    "angular": "1.5.8",
    "angular-animate": "1.5.8",
    "angular-aria": "1.5.8",
    "angular-material": "1.1.0",
    "angular-messages": "1.5.8",
    "angular-sanitize": "1.5.8",
    "lodash": "4.x",
    "openlayers": "3.18.2",
    "proj4": "2.x",
    "rx": "4.x",
    "rx-angular": "1.1.x"
  },
  "release": {
    "verifyConditions": "condition-circle"
  }
}
