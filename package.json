{
  "name": "os-elements",
  "version": "0.0.0-semantic-release",
  "description": "Reusable Angular1 components, from Ordnance Survey",
  "homepage": "http://ordnancesurvey.github.io/web-elements",
  "repository": {
    "type": "git",
    "url": "https://github.com/OrdnanceSurvey/web-elements.git"
  },
  "contributors": [
    "Craig Snyders <craig.snyders@os.uk>",
    "Sobieslaw Hrehorowicz <sobieslaw.hrehorowicz@os.uk>"
  ],
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/typings install & npm run tslint & tsc & webpack & webpack --config webpack.demos.config.js",
    "postinstall": "./node_modules/.bin/validate-commit-msg",
    "test": "./node_modules/.bin/karma start ./karma.config.js && ./node_modules/.bin/validate-commit-msg",
    "test-watcher": "./node_modules/.bin/karma start ./karma.config.js --no-single-run",
    "tslint": "./node_modules/.bin/tslint \"src/**/*.ts\" || true",
    "semantic-release": "./node_modules/.bin/semantic-release pre && npm publish && ./node_modules/.bin/semantic-release post",
    "docs-deploy": "tasks/docs-deploy.sh"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "angular-component": "0.0.3",
    "lodash": "^4.6.1",
    "material-design-lite": "^1.1.1",
    "nib": "^1.1.0",
    "rx": "^4.0.8",
    "rx-angular": "^1.1.3",
    "stylus-loader": "^1.4.3",
    "ts-loader": "^0.8.1"
  },
  "devDependencies": {
    "angular": "1.5.x",
    "angular-animate": "1.5.x",
    "angular-aria": "1.5.x",
    "angular-material": "1.x",
    "angular-messages": "1.5.x",
    "angular-mocks": "^1.5.x",
    "angular-sanitize": "1.5.x",
    "condition-circle": "^1.2.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "ghooks": "^1.2.1",
    "html-loader": "^0.4.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jade": "^1.11.0",
    "jade-html-loader": "0.0.3",
    "jade-loader": "^0.8.0",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.5",
    "karma-coveralls": "^1.1.2",
    "karma-jasmine": "^0.3.7",
    "karma-jasmine-matchers": "^2.0.2",
    "karma-nested-reporter": "^0.1.4",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-threshold-reporter": "^0.1.15",
    "karma-webpack": "^1.7.0",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "openlayers": "~3.8.2",
    "phantomjs-prebuilt": "^2.1.5",
    "semantic-release": "^4.3.5",
    "style-loader": "^0.13.0",
    "template-html-loader": "0.0.3",
    "tslint": "^3.6.0",
    "typescript": "1.7.5",
    "typings": "^0.6.5",
    "validate-commit-message": "^3.0.1",
    "validate-commit-msg": "^2.6.1",
    "webpack": "^1.12.13"
  },
  "peerDependencies": {
    "angular": "1.5.x",
    "angular-animate": "1.5.x",
    "angular-aria": "1.5.x",
    "angular-material": "^1.0.1",
    "angular-messages": "1.5.x",
    "angular-sanitize": "1.5.x",
    "openlayers": "~3.8.2"
  },
  "release": {
    "verifyConditions": "condition-circle"
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": ["feat", "fix", "docs", "style", "refactor", "perf", "test", "build", "ci", "chore"]
    }
  }
}
