osel-docs
  display flex
  flex 1
  flex-direction column

  table
    width 100%
    max-width 100%
    border-spacing 0
    border-radius 2px
    overflow hidden

  th
    background-color #4C9EF0
    color white

  th, td
    padding 12px 16px
    text-align left

  tr
    min-height 72px

  tr:nth-child(odd) td
    background-color #E3ECF5

  tr:nth-child(even) td
    background-color #D1DEEC


.osel-docs__page-content
  padding 24px
